{
  "compilerOptions": {
    "outDir": "./dist/",
    "rootDir": ".",
    "baseUrl": "src",
    "paths": {
      "@/*": ["*"]
    },
    "noImplicitAny": true,
    "module": "es6",
    "target": "ES2016",
    "jsx": "react-jsx",
    "allowJs": true,
    "moduleResolution": "node",
    "plugins": [{ "name": "typescript-plugin-css-modules" }]
  },
  "include": ["src", "cypress"]
}
